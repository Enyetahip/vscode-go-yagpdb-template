{
    "name": "gotemplate-yag",
    "displayName": "gotemplate-syntax-yag",
    "description": "Language support for YAGPDB's version of go text/template",
    "version": "0.2.0",
    "publisher": "Enyetahip",
    "repository": {
        "type": "git",
        "url": "https://github.com/enyetahip/vscode-go-yagpdb-template"
    },
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [ 
            {
                "id": "goyag",
                "aliases": [
                    "Golang Text Template",
                    "gotemplate",
                    "goyag"
                ],
                "extensions": [
                    ".gotmpl",
                    ".goyag",
                    ".goyagpdb"
                ],
                "configuration": "./goyag.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "goyag",
                "scopeName": "source.gotemplate",
                "path": "./syntaxes/goyag.tmLanguage"
            }
        ]
    }
}
