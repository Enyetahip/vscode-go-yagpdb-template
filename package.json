{
    "name": "gotemplate",
    "displayName": "gotemplate-syntax",
    "description": "Language support for go text/template",
    "version": "0.2.0",
    "publisher": "casualjim",
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "gotemplate",
                "aliases": [
                    "Golang Text Template",
                    "gotemplate"
                ],
                "extensions": [
                    ".gotmpl"
                ],
                "configuration": "./gotemplate.configuration.json"
            },
            {
                "id": "gohtml",
                "aliases": [
                    "Golang HTML Template",
                    "gohtml"
                ],
                "extensions": [
                    ".html",
                    ".gohtml"
                ],
                "configuration": "./gotemplate.configuration.json"
            },
            {
                "id": "goyaml",
                "aliases": [
                    "Golang YAML Template",
                    "goyaml"
                ],
                "extensions": [
                    ".yml.tpl",
                    ".yaml.tpl",
                    ".yml.tmpl",
                    ".yaml.tmpl"
                ],
                "configuration": "./gotemplate.configuration.json"
            },
            {
                "id": "gojson",
                "aliases": [
                    "Golang JSON Template",
                    "gojson"
                ],
                "extensions": [
                    ".json.tpl",
                    ".json.tmpl"
                ],
                "configuration": "./gotemplate.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gotemplate",
                "scopeName": "source.gotemplate",
                "path": "./syntaxes/gotemplate.tmLanguage"
            },
            {
                "language": "gohtml",
                "scopeName": "source.gohtml",
                "path": "./syntaxes/gohtml.tmLanguage"
            },
            {
                "language": "goyaml",
                "scopeName": "source.goyaml",
                "path": "./syntaxes/goyaml.tmLanguage"
            },
            {
                "language": "gojson",
                "scopeName": "source.gojson",
                "path": "./syntaxes/gojson.tmLanguage"
            }
        ]
    }
}
